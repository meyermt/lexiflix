<div class="container">
  <div class="page-header">
    <h1 class="network-name">Browse Users</h1>
      <h3>Total users: <%= @users.count %></h3>
  </div>


  <% @users.each do |u| %>

  <div class="row">

    <div class="col-sm-6 col-md-3">

      <h4>Name: <%= u.name %></h4>
      <p><u>Username</u>: <%= u.username %></p>
      <p><u>Email</u>: <%= u.email %></p>

    </div>
  </div>
  <% end %>
</div>
